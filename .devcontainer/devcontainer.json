{
  "name": "Go",
  "image": "cloudstateio/cloudstate-go-devcontainer",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined",
    "-v",
    "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "go.gopath": "/go"
  },
  "extensions": [
    "golang.go"
  ],
  "remoteUser": "vscode"
}